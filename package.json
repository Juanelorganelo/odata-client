{
  "name": "odata-client",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "release": "node scripts/release.js",
    "test": "jest 'tests(/.*)?/.*\\.spec\\.js'",
    "test:e2e": "jest 'e2e(/.*)?/.*\\.spec\\.js'",
    "test:all": "jest '(?:e2e|tests)(/.*)?/.*\\.spec\\.js'"
  },
  "engines": {
    "node": ">=12.3.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "camelcase": "^6.0.0",
    "lodash": "^4.17.19",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "chalk": "^4.1.0",
    "eslint": "^7.4.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "nock": "^13.0.2",
    "prettier": "^2.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 85,
        "lines": 85,
        "branches": 85,
        "statements": 85
      }
    }
  }
}
